{
  "name": "hangman",
  "type": "module",
  "scripts": {
    "dev": "yarn build && yarn start",
    "start": "node --es-module-specifier-resolution=node output/index.js",
    "build": "swc src/ -d output",
    "ts-check": "tsc --noEmit",
    "format": "prettier --cache --write ."
  },
  "dependencies": {
    "@discordjs/builders": "^1.6.5",
    "@discordx/pagination": "^3.4.1",
    "@discordx/utilities": "^5.2.1",
    "discord.js": "^14.13.0",
    "discordx": "^11.7.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.83",
    "@swc/helpers": "^0.5.2",
    "@types/express": "^4.17.18",
    "@types/node": "^20.5.9",
    "prettier": "^3.0.3",
    "typed-emitter": "^2.1.0",
    "typescript": "^5.2.2"
  }
